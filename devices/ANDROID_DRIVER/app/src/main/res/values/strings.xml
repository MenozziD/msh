    <string name="html_index">
               <![CDATA[
            <html>
                <head>
                    <meta charset="utf-8">
                    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

                    <!-- Bootstrap CSS -->
                    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

                    <!-- Font CSS -->
                    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">

                    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
                    <script src="https://code.jquery.com/jquery-3.4.0.min.js"></script>
                    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
                    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

                    <title>Not Found</title>
                </head>
                <script type="text/javascript">
                    var time_update=2000;
                    read_set("time_update");
                    read_set("device_name");
                    var myVar = setInterval(scanTimer, time_update);
                    $( document ).ready(read_all());
                    function sensor_cmd(str_cmd){
                        $.ajax({
                            url: "sensor",
                            type: "GET",
                            data: {type:str_cmd},
                            success: function(response,str_cmd){
                                var json = jQuery.parseJSON(response);
                                var res=json["response"];
                                var req=json["request"].split("=");
                                if (req[1]=="LIGHT")
                                    $("#LIGHT").val(res);
                                if (req[1]=="MAGNETIC_FIELD")
                                    $("#MAGNETIC_FIELD").val(res);
                                if (req[1]=="PROXIMITY")
                                    $("#PROXIMITY").val(res);
                            },
                            error: function(xhr){
                            }
                        });
                    }
                    function read_set(str_s){
                        $.ajax({
                            url: "settings",
                            type: "GET",
                            data: {str_set:str_s},
                            success: function(response){
                                var json = jQuery.parseJSON(response);
                                var res=json["response"];
                                var req=json["request"].split("=");
                                if (req[1]=="device_name")
                                    $("#device_name").text(res);
                                if (req[1]=="time_update")
                                    time_update=parseInt(res);
                            },
                            error: function(xhr){
                            }
                        });
                    }
                    function read_all()
                    {
                        sensor_cmd("LIGHT");
                        sensor_cmd("MAGNETIC_FIELD");
                        sensor_cmd("PROXIMITY");
                    }
                    function scanTimer()
                    {
                        read_all();
                    }
                </script>
                <body>
                    <div class="container">
                        <div class="row">
                            <div class="py-5 col-md-12 text-center">
                                <div class="error-template">
                                    <h1><p id="device_name"> </p></h1>
                                    <div class="form-row justify-content-center">
                                        <div class="py-3 input-group col-md-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text mb-2 " >Luminosita:</span>
                                            </div>
                                            <input id ="LIGHT" type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm" readonly>
                                        </div>
                                    </div>
                                    <div class="form-row justify-content-center">
                                        <div class="py-3 input-group col-md-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text mb-2 " >Prossimita:</span>
                                            </div>
                                            <input id ="PROXIMITY" type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm" readonly>
                                        </div>
                                    </div>
                                    <div class="form-row justify-content-center">
                                        <div class="py-3 input-group col-md-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text mb-2 " >Campo Magnetico:</span>
                                            </div>
                                            <input id ="MAGNETIC_FIELD" type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-sm" readonly>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </body>
            </html>
        ]]>

</string>

