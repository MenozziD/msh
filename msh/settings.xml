<settings>
	<porta>65177</porta>
	<lingua>IT</lingua>
	<timestamp>%Y-%m-%d %H:%M:%S</timestamp>
	<path>
		<ui>webui/</ui>
		<index>/static/page/index.html</index>
		<error>webui/page/error/</error>
		<db>db/system.db</db>
	</path>
	<log>
		<!-- Se valorizzato con None logga in console -->
		<filename>None</filename>
		<format>%(asctime)s|%(levelname)s|%(filename)s:%(lineno)s|%(message)s</format>
		<!-- debug, info, warning, error, critical -->
		<level>info</level>
	</log>
	<class_error>
		<error name="404">&lt;class 'webob.exc.HTTPNotFound'&gt;</error>
		<error name="405">&lt;class 'webob.exc.HTTPMethodNotAllowed'&gt;</error>
	</class_error>
	<shell_command>
		<ping>ping -c %s %s >%s</ping>
		<remove>sudo rm %s</remove>
		<ifconfig>ifconfig %s %s</ifconfig>
		<ifconfig1>ifconfig >%s</ifconfig1>
		<iwconfig>iwconfig</iwconfig>
		<pcwin_shutdown>net rpc shutdown -I %s -U %s >%s</pcwin_shutdown>
		<wake_on_lan>wakeonlan -i %s %s >%s</wake_on_lan>
		<net_scan>sudo nmap -sn %s/%s >%s</net_scan>
	</shell_command>
	<out_filename>
		<ping>pingout.txt</ping>
		<ifconfig>ip_subnet.txt</ifconfig>
		<pcwin_shutdown>winpcoffout.txt</pcwin_shutdown>
		<wake_on_lan>wolout.txt</wake_on_lan>
		<net_scan>netscanout.txt</net_scan>
	</out_filename>
	<string_success>
		<ping>0%</ping>
		<pcwin_shutdown>succeeded</pcwin_shutdown>
		<wake_on_lan>Sending magic packet</wake_on_lan>
	</string_success>
	<string_failure>
		<error>Error: </error>
		<generic>Error-%s: %s</generic>
		<ssh_login>SSH session failed on login!</ssh_login>
		<no_interface>Nessuna Interfaccia WiFi Rilevata!</no_interface>
	</string_failure>
	<command>
		<net>net_cmd</net>
		<ping>cmd_ping</ping>
		<radio_control>cmd_radioctrl</radio_control>
		<radio_status>cmd_radiostatus</radio_status>
		<pcwin_shutdown>cmd_pcwin_shutdown</pcwin_shutdown>
		<wake_on_lan>cmd_wakeonlan</wake_on_lan>
		<net_scan>cmd_netscan</net_scan>
	</command>
	<query>
		<select_tb_net_device>SELECT * FROM TB_NET_DEVICE;</select_tb_net_device>
		<select_tb_net_device_from_mac>SELECT * FROM TB_NET_DEVICE WHERE TB_NET_DEVICE.NET_MAC LIKE '%s';</select_tb_net_device_from_mac>
		<select_tb_net_device_tb_net_diz_cmd>SELECT * FROM TB_NET_DEVICE INNER JOIN TB_NET_DIZ_CMD ON TB_NET_DEVICE.NET_TYPE = TB_NET_DIZ_CMD.CMD_NET_TYPE WHERE TB_NET_DEVICE.NET_CODE LIKE '%s' AND TB_NET_DIZ_CMD.CMD_STR LIKE '%s'</select_tb_net_device_tb_net_diz_cmd>
		<select_tb_res_decode>SELECT * FROM TB_RES_DECODE WHERE RES_DEVICE_TYPE LIKE '%s' AND RES_DEVICE_SUBTYPE LIKE '%s' AND RES_COMMAND LIKE '%s' AND RES_LANG LIKE '%s' AND RES_VALUE == %s ;</select_tb_res_decode>
		<select_tb_net_device_type>SELECT * FROM TB_NET_DEVICE_TYPE;</select_tb_net_device_type>
        <select_net_command_for_type>SELECT * FROM TB_NET_DIZ_CMD WHERE CMD_NET_TYPE = '%s';</select_net_command_for_type>
		<insert_tb_net_device>INSERT INTO TB_NET_DEVICE (NET_CODE,NET_TYPE,NET_STATUS,NET_IP,NET_MAC,NET_MAC_INFO) VALUES ('%s','%s','%s','%s','%s','%s');</insert_tb_net_device>
		<update_tb_net_device>UPDATE TB_NET_DEVICE SET NET_CODE = '%s', NET_TYPE = '%s',NET_STATUS = '%s',NET_IP = '%s',NET_MAC_INFO = '%s' WHERE NET_MAC LIKE '%s';</update_tb_net_device>
	</query>
</settings>
